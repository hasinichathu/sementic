@prefix : <http://www.hospital.org/Ontologies.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.hospital.org/Ontologies/hospital.owl> .

<http://www.hospital.org/Ontologies/hospital.owl> rdf:type owl:Ontology ;
                                                  
                                                  rdfs:comment "This is about a hospital ontology" .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.hospital.org/Ontologies.owl#admittedTo

:admittedTo rdf:type owl:ObjectProperty ;
            
            rdfs:range :Hospital ;
            
            rdfs:domain :Patient ;
            
            rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#advicesTo

:advicesTo rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           
           rdfs:domain :Doctor ;
           
           rdfs:range :Staff ;
           
           rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#check

:check rdf:type owl:ObjectProperty ;
       
       rdfs:range :Conflict ;
       
       rdfs:domain :LabrotaryStaff ;
       
       rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasConcernOn

:hasConcernOn rdf:type owl:ObjectProperty ;
              
              rdfs:range :Alert ;
              
              rdfs:domain :Doctor ;
              
              rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasEvents

:hasEvents rdf:type owl:ObjectProperty ;
           
           rdfs:range :Event ;
           
           rdfs:domain :Hospital ;
           
           rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasHospital

:hasHospital rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:domain :Doctor ;
             
             rdfs:range :Hospital ;
             
             rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasPeople

:hasPeople rdf:type owl:ObjectProperty ;
           
           rdfs:domain :Hospital ;
           
           rdfs:range :People ;
           
           rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasPhysicalResources

:hasPhysicalResources rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain :Hospital ;
                      
                      rdfs:range :PhysicalResource ;
                      
                      rdfs:subPropertyOf :hasWork .



###  http://www.hospital.org/Ontologies.owl#hasPolicy

:hasPolicy rdf:type owl:ObjectProperty ;
           
           rdfs:domain :Hospital ;
           
           rdfs:range :Policy ;
           
           rdfs:subPropertyOf :hasWork ;
           
           owl:inverseOf :isPolicyOf .



###  http://www.hospital.org/Ontologies.owl#hasServices

:hasServices rdf:type owl:ObjectProperty ;
             
             rdfs:domain :Hospital ;
             
             rdfs:range :Service ;
             
             rdfs:subPropertyOf :hasWork ;
             
             owl:inverseOf :isServiceOf .



###  http://www.hospital.org/Ontologies.owl#hasWork

:hasWork rdf:type owl:ObjectProperty .



###  http://www.hospital.org/Ontologies.owl#isHospitalActionOf

:isHospitalActionOf rdf:type owl:ObjectProperty ;
                    
                    rdfs:range :Hospital ;
                    
                    rdfs:domain :Procedure ;
                    
                    rdfs:subPropertyOf :isWorkOf ;
                    
                    owl:inverseOf :perform .



###  http://www.hospital.org/Ontologies.owl#isPolicyOf

:isPolicyOf rdf:type owl:ObjectProperty ;
            
            rdfs:range :Hospital ;
            
            rdfs:domain :Policy ;
            
            rdfs:subPropertyOf :isWorkOf .



###  http://www.hospital.org/Ontologies.owl#isServiceOf

:isServiceOf rdf:type owl:ObjectProperty ;
             
             rdfs:range :Hospital ;
             
             rdfs:domain :Service ;
             
             rdfs:subPropertyOf :isWorkOf .



###  http://www.hospital.org/Ontologies.owl#isWorkOf

:isWorkOf rdf:type owl:ObjectProperty .



###  http://www.hospital.org/Ontologies.owl#perform

:perform rdf:type owl:ObjectProperty ;
         
         rdfs:domain :Hospital ;
         
         rdfs:range :Procedure ;
         
         rdfs:subPropertyOf :hasWork .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.hospital.org/Ontologies.owl#hasAdmittedDate

:hasAdmittedDate rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 
                 rdfs:subPropertyOf owl:topDataProperty .



###  http://www.hospital.org/Ontologies.owl#hasBeds

:hasBeds rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty .



###  http://www.hospital.org/Ontologies.owl#hasEmail

:hasEmail rdf:type owl:DatatypeProperty ;
          
          rdfs:subPropertyOf owl:topDataProperty .



###  http://www.hospital.org/Ontologies.owl#hasHospitalChair

:hasHospitalChair rdf:type owl:DatatypeProperty ;
                  
                  rdfs:subPropertyOf owl:topDataProperty .



###  http://www.hospital.org/Ontologies.owl#hasWardNumber

:hasWardNumber rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               
               rdfs:subPropertyOf owl:topDataProperty .



###  http://www.hospital.org/Ontologies.owl#isHospitalPhone

:isHospitalPhone rdf:type owl:DatatypeProperty ;
                 
                 rdfs:subPropertyOf owl:topDataProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.hospital.org/Ontologies.owl#Addmitance

:Addmitance rdf:type owl:Class ;
            
            rdfs:subClassOf :Procedure .



###  http://www.hospital.org/Ontologies.owl#Alert

:Alert rdf:type owl:Class ;
       
       rdfs:subClassOf :Event .



###  http://www.hospital.org/Ontologies.owl#Anesthesiology

:Anesthesiology rdf:type owl:Class ;
                
                rdfs:subClassOf :Service ;
                
                owl:disjointWith :CancerCenterService .



###  http://www.hospital.org/Ontologies.owl#AyurvedicHospital

:AyurvedicHospital rdf:type owl:Class ;
                   
                   rdfs:subClassOf :NamedHospital ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPolicy ;
                                     owl:allValuesFrom :Government
                                   ] ;
                   
                   rdfs:comment "These hospitals have local treatments" .



###  http://www.hospital.org/Ontologies.owl#CancerCenterService

:CancerCenterService rdf:type owl:Class ;
                     
                     rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#CardiologyService

:CardiologyService rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#ChargeFee

:ChargeFee rdf:type owl:Class ;
           
           rdfs:subClassOf :Event .



###  http://www.hospital.org/Ontologies.owl#ClinicalService

:ClinicalService rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#Conflict

:Conflict rdf:type owl:Class ;
          
          rdfs:subClassOf :Event .



###  http://www.hospital.org/Ontologies.owl#Discharge

:Discharge rdf:type owl:Class ;
           
           rdfs:subClassOf :Procedure .



###  http://www.hospital.org/Ontologies.owl#Doctor

:Doctor rdf:type owl:Class ;
        
        rdfs:subClassOf :Staff .



###  http://www.hospital.org/Ontologies.owl#Drug

:Drug rdf:type owl:Class ;
      
      rdfs:subClassOf :PhysicalResource .



###  http://www.hospital.org/Ontologies.owl#DrugConflict

:DrugConflict rdf:type owl:Class ;
              
              rdfs:subClassOf :Conflict .



###  http://www.hospital.org/Ontologies.owl#EmergencyService

:EmergencyService rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#Equipment

:Equipment rdf:type owl:Class ;
           
           rdfs:subClassOf :PhysicalResource .



###  http://www.hospital.org/Ontologies.owl#Event

:Event rdf:type owl:Class .



###  http://www.hospital.org/Ontologies.owl#ForProfitHospital

:ForProfitHospital rdf:type owl:Class ;
                   
                   rdfs:subClassOf :NamedHospital ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :EmergencyService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :CardiologyService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPolicy ;
                                     owl:allValuesFrom :GroupOfPeople
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :SurgeryService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :CancerCenterService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :IntensiveCareService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :InfantService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPolicy ;
                                     owl:allValuesFrom :ProfitEarning
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :Anesthesiology
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasServices ;
                                     owl:someValuesFrom :RadiologyService
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasEvents ;
                                     owl:allValuesFrom :ChargeFee
                                   ] ;
                   
                   rdfs:comment "A hospital which does not expect a profit." ,
                                "A hospital which charge fee for every hospital facility" .



###  http://www.hospital.org/Ontologies.owl#Government

:Government rdf:type owl:Class ;
            
            rdfs:subClassOf :OwnedBy ;
            
            owl:disjointWith :GroupOfPeople .



###  http://www.hospital.org/Ontologies.owl#GovernmentHospital

:GovernmentHospital rdf:type owl:Class ;
                    
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:intersectionOf ( :Hospital
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasPolicy ;
                                                                 owl:allValuesFrom :Government
                                                               ]
                                                             )
                                        ] .



###  http://www.hospital.org/Ontologies.owl#GroupOfPeople

:GroupOfPeople rdf:type owl:Class ;
               
               rdfs:subClassOf :OwnedBy .



###  http://www.hospital.org/Ontologies.owl#GynecologyService

:GynecologyService rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#HighFacilityHospital

:HighFacilityHospital rdf:type owl:Class ;
                      
                      owl:equivalentClass [ rdf:type owl:Class ;
                                            owl:intersectionOf ( :Hospital
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasBeds ;
                                                                   owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                        owl:onDatatype xsd:integer ;
                                                                                        owl:withRestrictions ( [ xsd:minInclusive 1000
                                                                                                               ]
                                                                                                             )
                                                                                      ]
                                                                 ]
                                                               )
                                          ] .



###  http://www.hospital.org/Ontologies.owl#Hospital

:Hospital rdf:type owl:Class ;
          
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPeople ;
                            owl:someValuesFrom :People
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :perform ;
                            owl:someValuesFrom :Procedure
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasPhysicalResources ;
                            owl:someValuesFrom :PhysicalResource
                          ] .



###  http://www.hospital.org/Ontologies.owl#InfantService

:InfantService rdf:type owl:Class ;
               
               rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#IntensiveCareService

:IntensiveCareService rdf:type owl:Class ;
                      
                      rdfs:subClassOf :Service .



###  http://www.hospital.org/Ontologies.owl#LabrotaryStaff

:LabrotaryStaff rdf:type owl:Class ;
                
                rdfs:subClassOf :Staff .



###  http://www.hospital.org/Ontologies.owl#LowFacilityHospital

:LowFacilityHospital rdf:type owl:Class ;
                     
                     owl:equivalentClass [ rdf:type owl:Class ;
                                           owl:intersectionOf ( :Hospital
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasBeds ;
                                                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                       owl:onDatatype xsd:integer ;
                                                                                       owl:withRestrictions ( [ xsd:maxInclusive 500
                                                                                                              ]
                                                                                                            )
                                                                                     ]
                                                                ]
                                                              )
                                         ] .



