                               [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasEvents ;
                                                        owl:allValuesFrom :ChargeFee
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasPolicy ;
                                                        owl:allValuesFrom [ owl:intersectionOf ( :GroupOfPeople
                                                                                                 :ProfitEarning
                                                                                               ) ;
                                                                            rdf:type owl:Class
                                                                          ]
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :CancerCenter
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :Cardiology
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :ClinicalService
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :Emergency
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :Gynecology
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :IntensiveCare
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasServices ;
                                 owl:someValuesFrom :Radiology
